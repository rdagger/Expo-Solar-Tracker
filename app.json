{
  "expo": {
    "name": "Solar-Tracker",
    "description": "Display Solar Rest API Data",
    "slug": "solar",
    "privacy": "unlisted",
    "platforms": ["ios", "android"],
    "version": "1.0.40",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
